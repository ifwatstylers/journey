@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model journey.Models.DriverListModel

<h1>Student</h1>

<table>
    @foreach (var driver in Model.DriverID.ToString())
    {
        <tr>
            <td>@driver.ToString()</td>
        </tr>
    }
    @foreach (var driver in Model.Email.ToString())
    {
        <tr>
            <td>@driver.ToString()</td>
        </tr>
    }
</table>